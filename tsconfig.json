{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },  

  "typedocOptions": {    
    "entryPoints": ["./src/app/services/administrator.service.ts",
                    "./src/app/services/auth.service.ts",
                    "./src/app/services/cart.service.ts",
                    "./src/app/services/firestorage.service.ts",
                    "./src/app/services/firestore.service.ts",
                    "./src/app/auth/login/login.component.ts",
                    "./src/app/auth/register/register.component.ts",
                    "./src/app/pages/add-product/add-product.component.ts",
                    "./src/app/pages/administrators/list-administrator/list-administrator.component.ts",
                    "./src/app/pages/cart/cart.component.ts",
                    "./src/app/pages/checkout/checkout.component.ts",
                    "./src/app/pages/home/home.component.ts",
                    "./src/app/pages/my-orders/my-orders.component.ts",
                    "./src/app/pages/my-products/my-products.component.ts",
                    "./src/app/pages/product-details/product-details.component.ts",
                    "./src/app/pages/profile/profile.component.ts",
                    "./src/app/pages/see-products/see-products.component.ts",
                    "./src/app/models/models.ts",
                    "./src/app/guards/auth.guard.ts",        
                    "./src/app/guards/logueado.guard.ts",
                    "./src/app/pages/administrators/list-administrator/administratorInterface.ts"],
    "out": "docs",    
  }
} 